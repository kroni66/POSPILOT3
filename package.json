{
  "name": "pos-sco-dashboard",
  "version": "2.1.0",
  "description": "POS and SCO Dashboard",
  "author": "Michael Corsa",
  "main": "public/electron.js",
  "homepage": "./",
  "icon": "src/styles/lady.ico",
  "scripts": {
    "start": "react-scripts start",
    "build": "electron-builder",
    "dev": "electron .",
    "package": "electron-builder --win --x64",
    "react-build": "react-scripts build",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@monaco-editor/react": "^4.6.0",
    "@tanstack/react-virtual": "^3.10.7",
    "@types/codemirror": "^5.60.15",
    "@types/react-syntax-highlighter": "^15.5.13",
    "archiver": "^7.0.1",
    "codemirror": "^5.65.17",
    "extract-zip": "^2.0.1",
    "fast-xml-parser": "^4.4.1",
    "fix-esm": "^1.0.1",
    "monaco-editor": "^0.51.0",
    "node-websockify": "^1.0.3",
    "path-browserify": "^1.0.1",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-codemirror2": "^8.0.0",
    "react-complex-tree": "^2.4.5",
    "react-data-grid": "^7.0.0-beta.46",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-scripts": "^5.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "react-virtualized": "^9.22.3",
    "react-virtualized-auto-sizer": "^1.0.24",
    "react-vnc": "^1.0.0",
    "@mrblenny/react-flow-chart": "0.0.14",
    "styled-components": "6.1.13",
    "react-window": "^1.8.10",
    "uuid": "^10.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-react": "^7.24.7",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.5.0",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "@types/react-virtualized": "^9.21.30",
    "@types/react-virtualized-auto-sizer": "^1.0.4",
    "@types/react-window": "^1.8.8",
    "babel-loader": "^9.1.3",
    "electron": "^31.4.0",
    "electron-builder": "^24.13.3",
    "electron-installer-zip": "^0.1.2",
    "typescript": "^4.9.5"
  },
  "build": {
    "appId": "com.yourcompany.poscosodashboard",
    "productName": "POS SCO Dashboard",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json",
      {
        "from": "assets",
        "to": "assets",
        "filter": [
          "**/*"
        ]
      }
    ],
    "extraResources": [
      {
        "from": "assets",
        "to": "assets",
        "filter": [
          "**/*"
        ]
      }
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": "7z",
      "artifactName": "pospilot.7z",
      "icon": "src/styles/lady.ico",
      "extraFiles": [
        {
          "from": "node_modules/node-expose-sspi/build/Release/",
          "to": "resources/app/node_modules/node-expose-sspi/build/Release/",
          "filter": ["*.node"]
        }
      ]
    },
    "publish": [
      {
        "provider": "github",
        "owner": "kroni66",
        "repo": "POSPILOT3",
        "token": "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"
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "edge >= 79"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 edge version"
    ]
  },
  "browser": {
    "fs": false,
    "os": false,
    "path": false
  },
  "asar": false,
  "forceCodeSigning": false,
  "signAndEditExecutable": false,
  "buildDependenciesFromSource": true,
  "npmRebuild": false
}
